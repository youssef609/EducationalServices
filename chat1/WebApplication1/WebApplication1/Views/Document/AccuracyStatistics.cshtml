@model IEnumerable<dynamic>

@{
    ViewData["Title"] = "Accuracy Statistics";
}
<div class="container mt-5">
<h1>Accuracy Statistics</h1>
    <div class="card">

        <table class="table table-hover mt-3">
             <thead>
                <tr>
                    <th>Model Name</th>
                    <th>Total Questions</th>
                    <th>Correct Answers</th>
                    <th>Accuracy Percentage</th>
                    <th>Average Time Taken (ms)</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var stat in Model)
                {
                    <tr>
                        <td>@stat.ModelName</td>
                        <td>@stat.TotalQuestions</td>
                        <td>@stat.CorrectAnswers</td>
                        <td>@stat.AccuracyPercentage.ToString("F2")%</td>
                        <td>@stat.AverageTimeTaken.ToString("F2")</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
